{
  "typescript": true,
  "commands": [
    "./commands",
    "@adonisjs/core/build/commands/index.js",
    "@adonisjs/repl/build/commands",
    "@adonisjs/lucid/build/commands",
    "@adonisjs/bouncer/build/commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts",
    "Lib": "lib"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    {
      "file": "./start/wsKernel",
      "environment": ["web", "test"],
      "optional": true
    },
    {
      "file": "./start/socket",
      "environment": ["web", "test"],
      "optional": true
    },
    "./start/orm",
    "./start/bouncer",
    {
      "file": "./start/webPush",
      "environment": ["web", "test"],
      "optional": true
    }
  ],
  "providers": [
    "./providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/auth",
    "@ruby184/adonis-socket.io",
    "@adonisjs/bouncer"
  ],
  "metaFiles": ["ssl/*"],
  "aceProviders": ["@adonisjs/repl"],
  "tests": {
    "suites": [
      {
        "name": "functional",
        "files": ["tests/functional/**/*.spec(.ts|.js)"],
        "timeout": 60000
      }
    ]
  },
  "testProviders": ["@japa/preset-adonis/TestsProvider"],
  "wsExceptionHandlerNamespace": "App/Exceptions/WsHandler"
}
